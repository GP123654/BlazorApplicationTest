    }

    .container18{
        min-height:100vh;
        display:flex;
        justify-content:center;
        align-items: center;
        flex-direction: column
    }

    .container18 a{
        position:relative;
        width: 300px;
        height: 75px;
        margin: 10px 0;
        font-size:24px;
        color: black;
        display: inline-flex;
        justify-content:center;
        align-items:center;
        text-decoration:none;
        transition:0.5s;
    }

    .container18 a:hover{
        color:white;
    }

    .container18 a:before {
        content:'';
        position: absolute;
        top:0;
        left:0;
        width:0;
        height:100%;
        background: url(/images/btn-bg1.png);
        background-size:cover;
        transform-origin: left;
        transition: 0.8s;
    }

    .container18 a:nth-child(2):before {
        background: url(/images/btn-bg2.png);
        background-size: cover;
    }

    .container18 a:hover:before{
          width: 100%;
    }

    .container18 a span{
        position:relative;
        z-index:auto;
    }



