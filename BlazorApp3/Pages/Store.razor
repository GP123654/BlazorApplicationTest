    .body19 {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
    }

    .image-container {
        position: relative;
        height:100vh;
    }

    .bg-image,
    .overlay-image {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        object-fit: cover;
    }

    .overlay-image {
        z-index: 1;
        mix-blend-mode: screen;
    }

    section {
        position: relative;
        width: 100%;
        min-height: 100vh;
    }

    .body19 .image-container h1{
        text-align: center;
        padding-top: 40px;
        font-size: 70px;
        font-family: "Franklin Gothic Medium", "Arial Narrow", Arial, sans-serif;
        text-transform: uppercase;
        color: mediumpurple;
        z-index:999999;
    }




    .banner-area8 {
        width: 100%;
        height: 600px;
        position: relative;
        top: 0;
        background-image: url("../Images/StoreHead1.jpg");
        -webkit-background-size: cover;
        background-size: cover;
        background-position: center center;
    }

    .overlay-image2 {
        position: relative;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background-image: url("/images/splash.jpg");
        background-size: cover;
        background-repeat: no-repeat;
        mix-blend-mode: screen;
    }

    .banner-area8 h2 {
        text-align: center;
        padding-top: -80px;
        font-size: 70px;
        font-family: "Franklin Gothic Medium", "Arial Narrow", Arial, sans-serif;
        text-transform: uppercase;
        color: mediumpurple;
        z-index:999;
    }




</style>


